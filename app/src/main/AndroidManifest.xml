<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uw.team5.huskymathchallenge" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".CreateKeyHash"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_ID" />

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".CreateNewUserActivity" >
        </activity>
        <activity android:name=".Menu" >
        </activity>
        <activity android:name=".AlgebraCategory.AlgebraActivity" >
        </activity>
        <activity android:name=".AllAboutMath.AllAboutMath" >
        </activity>
        <activity android:name=".AllAboutMath.ResultAllMathActivity" >
        </activity>
        <activity android:name=".InstructionActivity" >
        </activity>
        <activity android:name=".GeometryCategory.ResultGeometryActivity" >
        </activity>
        <activity android:name=".GeometryCategory.ViewScoreActivity" >
        </activity>
        <activity android:name=".AlgebraCategory.ResultAlgebraQuizActivity" >
        </activity>
        <activity android:name=".AlgebraCategory.ViewAlgebraScoreActivity" >
        </activity>
        <activity android:name=".CalculusCategory.CalculusQuizActivity" >
        </activity>
        <activity android:name=".CalculusCategory.ResultCalculusQuizActivity" >
        </activity>
        <activity android:name=".GeometryCategory.GeometryQuizActivity" >
        </activity>
    </application>

</manifest>
